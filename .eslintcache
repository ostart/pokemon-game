n onClick={onClickHandler}>\r\n                Return Home\r\n            </button>\r\n            <Layout id=\"cards\" title=\"Cards\" colorBg=\"#202736\" colorTitle=\"#FEFEFE\">\r\n                <div className={s.flex}><button onClick={addNewPokemonHandler}>ADD NEW POKEMON</button></div>\r\n                <div className={s.flex}>\r\n                {\r\n                    Object.entries(pokemons).map(([k, v]) => <PokemonCard \r\n                        key={k} \r\n                        id={v.id} \r\n                        name={v.name} \r\n                        type={v.type} \r\n                        values={v.values} \r\n                        img={v.img} \r\n                        onCardClick={onCardClickHandler}\r\n                        isActive={v.isActive}\r\n                        />)\r\n                }\r\n                </div>\r\n            </Layout>\r\n        </>\r\n    );\r\n};\r\n\r\nexport default GamePage;","C:\\Users\\aostaschenko\\source\\repos\\pokemon-game\\src\\routes\\Home\\index.js",[],"C:\\Users\\aostaschenko\\source\\repos\\pokemon-game\\src\\service\\firebase.js",[],"C:\\Users\\aostaschenko\\source\\repos\\pokemon-game\\src\\components\\Layout\\index.js",[],"C:\\Users\\aostaschenko\\source\\repos\\pokemon-game\\src\\components\\NavBar\\index.js",[],"C:\\Users\\aostaschenko\\source\\repos\\pokemon-game\\src\\components\\PokemonCard\\index.js",[],"C:\\Users\\aostaschenko\\source\\repos\\pokemon-game\\src\\components\\Menu\\index.js",[],"C:\\Users\\aostaschenko\\source\\repos\\pokemon-game\\src\\components\\Header\\index.js",[],"C:\\Users\\aostaschenko\\source\\repos\\pokemon-game\\src\\context\\firebaseContext.js",[],{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","severity":1,"message":"76","line":25,"column":8,"nodeType":"77","endLine":25,"endColumn":10,"suggestions":"78"},"no-native-reassign",["79"],"no-negated-in-lhs",["80"],"react-hooks/exhaustive-deps","React Hook useEffect has a mis